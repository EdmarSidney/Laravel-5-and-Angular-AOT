{
  "private": true,
  "scripts": {
    "ngc": "ngc",
    "cleanup": "rimraf resources/assets/angular/**/*.ngfactory.ts resources/assets/angular/**/*.ngstyle.ts resources/assets/angular/**/*.ngsummary.json resources/assets/angular/ngfactory resources/assets/angular/aot",
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules",
    "watch-poll": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --watch-poll --progress --hide-modules",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot",
    "production": "npm run cleanup && npm run ngc && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules"
  },
  "dependencies": {
    "@angular/common": "^4.0.3",
    "@angular/compiler": "^4.0.3",
    "@angular/core": "^4.0.3",
    "@angular/forms": "^4.0.3",
    "@angular/http": "^4.0.3",
    "@angular/platform-browser": "^4.0.3",
    "@angular/platform-browser-dynamic": "^4.0.3",
    "@angular/router": "^4.0.3",
    "@angular/upgrade": "^4.0.3",
    "@angular/animations": "^4.0.3",
    "core-js": "2.4.1",
    "rxjs": "5.3.0",
    "zone.js": "0.8.8"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.3",
    "@angular/platform-server": "^4.0.3",
    "@types/core-js": "0.9.41",
    "@types/node": "7.0.13",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "typescript": "2.2.2",
    "to-string-loader": "1.1.5",
    "axios": "^0.16.1",
    "bootstrap-sass": "^3.3.7",
    "cross-env": "^4.0.0",
    "jquery": "^3.1.1",
    "laravel-mix": "0.*",
    "lodash": "^4.17.4",
    "vue": "2.2.6"
  }
}
